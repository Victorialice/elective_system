<h1>Listing Users</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Number</th>
    <th>Password</th>
  </tr>

  <% @users.each do |user| %>
    <tr>
      <td><%= user.name %></td>
      <td><%= user.number %></td>
      <td><%= user.password%></td>
    </tr>
  <% end %>
  <%= link_to 'New user', user_new_path %>
</table>
